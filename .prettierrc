{
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "semi": false,
  "trailingComma": "none",
  "useTabs": true,
  "printWidth": 88,
  "vueIndentScriptAndStyle": true,
  "singleAttributePerLine": false,
  "quoteProps": "as-needed",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "strict"
}